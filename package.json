{
  "name": "hurricane",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "webpack",
    "prebuild": "rm -rf ./dist",
    "postbuild": "yarn html && yarn ver-qstr && yarn manifest && yarn data && yarn img && yarn css",
    "html": "cp ./src/index.html ./dist",
    "ver-qstr": "v=$npm_package_version; sed \"s/%ver%/${v}/g\" ./dist/index.html > tmpfile && mv tmpfile ./dist/index.html",
    "manifest": "cp ./src/manifest.webmanifest ./dist",
    "data": "cp -r ./src/data ./dist/data",
    "img": "cp -r ./src/img ./dist/img",
    "css": "cp -r ./src/css ./dist/css && cp -r ./node_modules/@timkeane/nyc-lib/dist/css/ ./dist/css/"
  },
  "author": "Tim Keane <t.d.keane@gmail.com> (https://timkeane.github.io/)",
  "license": "ISC",
  "dependencies": {
    "@timkeane/nyc-lib": "file:../nyc-lib/timkeane-nyc-lib-v0.8.58-SNAPSHOT.tgz",
    "es6-promise": "^4.2.4",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.3.1",
    "ol": "^4.6.5",
    "papaparse": "^4.4.0",
    "proj4": "^2.4.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.4",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-preset-env": "^1.7.0",
    "canvas-prebuilt": "^1.6.5-prerelease.1",
    "clean-webpack-plugin": "^0.1.19",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.1.0",
    "postcss-clean": "^1.1.0",
    "postcss-css-variables": "^0.8.1",
    "postcss-import": "^11.1.0",
    "webpack": "^4.10.2",
    "webpack-cli": "^2.1.4"
  },
  "jest": {
    "verbose": true,
    "automock": false,
    "transformIgnorePatterns": [],
    "setupFiles": []
  }
}
